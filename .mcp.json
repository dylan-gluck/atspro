{
	"mcpServers": {
		"playwright": {
			"command": "npx",
			"args": ["@playwright/mcp@latest"]
		},
		"postgres": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-postgres",
				"postgresql://postgres:postgres@localhost/atspro"
			]
		},
		"shadcn-ui": {
			"command": "npx",
			"args": ["@jpisnice/shadcn-ui-mcp-server", "--framework", "svelte"]
		}
	}
}
