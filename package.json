		"@testing-library/user-event": "^14.6.1",
		"@types/bun": "^1.2.20",
		"@vitest/browser": "^3.2.3",
		"@vitest/coverage-v8": "^3.2.4",
		"ai": "^5.0.23",
		"bits-ui": "^2.8.6",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"formsnap": "^2.0.1",
