		"svelte": "^5.0.0",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.26.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.3.7",
		"typescript": "^5.0.0",
